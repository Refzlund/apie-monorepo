{
	"name": "@api/pipe",
	"version": "0.1.0",
	"description": "Library template that uses Vite for building, Vitest for testing, Changesets for versioning, renovate for upgrading packages.",
	"keywords": [],
	"author": "Arthur Refslund @Refzlund",
	"license": "MIT",
	"repository": "github:refzlund/api-monorepo",
	"homepage": "https://github.com/refzlund/api-monorepo/packages/@api-pipe",
	"bugs": {
		"url": "https://github.com/refzlund/api-monorepo/labels/bug"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"lint": "eslint **/*.{js,ts,cjs}",
		"typecheck": "vitest typecheck -t \".*\\.ts\"",
		"test": "bun test",
		"precommit": "pnpm run lint && pnpm run typecheck && pnpm run test",
		"package": "bun run ./vite"
	},
	"dependencies": {
		"@api/utility": "^1.0.0"
	},
	"devDependencies": {
		"@api/utility": "workspace:1.0.0",
		"@api/responses": "workspace:*"
	},
	"peerDependencies": {
		"@api/responses": "*"
	}
}